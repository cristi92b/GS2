<table class="table table-hover"
	<tr>
		<td><b>
			Title
		</b>
		</td>
		
		<td><b>
			Album
		</b>
		</td>
		
		<td><b>
			Artist
		</b>
		</td>
		
		<td><b>
			Genre
		</b>
		</td>
		
		<td><b>
			Track
		
		<td><b>
			Year
		</b>
		</td>
		
		<td></td>
		<td></td>
		</td></td>
		<td></td>
		<td></td>
	</tr>
<% @songs.each do |song| %>
	<tr>
		<td>
			<%= song.title %>
		</td>
		<td>
			<%= song.album %>
		</td>
		<td>
			<%= song.artist %>
		</td>
		<td>
			<%= song.genre %>
		</td>
		<td>
			<%= song.track %>
		</td>
		<td>
			<%= song.year %>
		</td>
		<td>
			<%= link_to "Edit", edit_dashboard_path(song.id) %>
		</td>
		<td>
			<%= link_to "Play", dashboard_path(song.id) %>
		</td>

		<td>
			<%= link_to "Download", "/dashboard/download?id=#{song.id}" %>
		</td>
		<td>
			<%= link_to "Destroy", dashboard_path(song.id) , :method => :delete  %>
		</td>
	</tr>
<% end %>
</table>
